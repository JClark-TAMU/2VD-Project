<!-- (Signed in) Home Page -->
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Projects - Brand</title>
    <link rel="stylesheet" href="app/assets/stylesheets/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato:300,400,700">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Archivo&display=swap">
    <link rel="stylesheet" href="app/assets/fonts/ionicons.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.6.1/css/pikaday.min.css">
</head>

<body style="background: rgb(252,249,246);">
    <nav class="navbar navbar-dark navbar-expand-lg fixed-top bg-white portfolio-navbar gradient">
        <div class="container"><a class="navbar-brand logo" href="#">
            <%= image_tag ("2VD-logo.png"), :size => "30"%></a>
            <button class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#navbarNav">
                <span class="visually-hidden">Toggle navigation</span>
                <span class="navbar-toggler-icon"></span>
            </button>
            <%# <button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navbarNav">
                <span class="visually-hidden">Toggle navigation</span>
                <span class="navbar-toggler-icon"></span>
                </button> %>
            <div class="collapse navbar-collapse" id="navbarNav" style="text-align: left;">
                <ul class="navbar-nav ms-auto" style="margin-right:auto">
                    <li class="nav-item"><a class="nav-link" href="<%=root_path%>">Home</a></li>
                    <li class="nav-item"><a class="nav-link active" href="<%= officer_path%>">Officers</a></li>
                    <li class="nav-item"><a class="nav-link" href="<%= users_path %>">Members</a></li>
                    <li class="nav-item"><a class="nav-link" href="<%= images_path %>">Images</a></li>
                    <li class="nav-item dropdown">
                        <a class="dropdown-toggle nav-link" aria-expanded="false" data-toggle="dropdown" href="#">Account</a>
                        <div class="dropdown-menu show">
                            <a class="dropdown-item" href="<%= user_path(User.find_by(email: current_admin.email)) %>">Profile </a>
                            <a class="dropdown-item" href="<%=portfolio_path(User.find_by(email: current_admin.email).portfolio) %>">Portfolio</a>
                        </div>
                    </li>
                </ul>
    
                <% if not User.find_by(email: current_admin.email).portfolio %>
                    <%= link_to 'Create Account', edit_user_path(User.find_by(email: current_admin.email)) %> <br>
                <% end %>
                <button class="btn btn-dark" onclick="location.href='<%= destroy_admin_session_path %>'" type="button">Sign Out</button>
                <!--<button class="btn btn-primary" onclick="location.href='<%= edit_user_path(User.find_by(email: current_admin.email)) %>'" type="button">Create Account</button>-->
            </div>
        </div>
    </nav>

<div> 
    <% if not User.find_by(email: current_admin.email).portfolio %>
        <h4>If you have not made an account yet and plan on being a member, follow the link below. </h4>
    <%= link_to 'Create Account Here', edit_user_path(User.find_by(email: current_admin.email)) %> <br>
    <% end %>
</div>

    <main class="page projects-page" style="padding-top: 6%;">
        <section class="portfolio-block projects-cards" style="padding-top: 5px;padding-bottom: 50px;">
            <h1 style="text-align: center;margin-top: 0%;margin-bottom: 1%;"><strong>Current Prompt</strong></h1>
            <div class="container" style="padding: 0px;">
                <div class="heading"></div>
                <div class="row">
                    <div class="col">
                        <div class="carousel slide" data-bs-ride="carousel" id="carousel-1">
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <%= image_tag "image5.jpg", style: 'height:50vw;width:100vw;margin-bottom:20px;' %>
                                </div>
                                <div class="carousel-item">
                                    <%= image_tag "avatar.jpg", style: 'height:50vw;width:100vw;margin-bottom:20px;' %>
                                </div>
                                <div class="carousel-item">
                                    <%= image_tag "2VD-header.jpg", style: 'height:50vw;width:100vw;margin-bottom:20px;' %>
                                </div>
                            </div>
                            <div>
                                <a class="carousel-control-prev" href="#carousel-1" role="button" data-bs-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="visually-hidden">Previous</span>
                                </a>
                                <a class="carousel-control-next" href="#carousel-1" role="button" data-bs-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="visually-hidden">Next</span>
                                </a>
                            </div>
                            <ol class="carousel-indicators">
                                <li data-bs-target="#carousel-1" data-bs-slide-to="0" class="active"></li>
                                <li data-bs-target="#carousel-1" data-bs-slide-to="1"></li>
                                <li data-bs-target="#carousel-1" data-bs-slide-to="2"></li>
                            </ol>
                        </div>
                    </div>
                </div>
                <div>
                    <div class="row">
                        <div class="col" style="padding-left: 45.5%;padding-top: 30px;">
                        <button class="btn btn-dark" type="button" style="padding-right: 5%;background: rgb(124,57,152);padding-left: 5%;padding-bottom: 2%;padding-top: 2%;font-weight: bold;">Submit</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h1 style="text-align: center;margin-top: 40px;margin-bottom: 30px;"><strong>Previous Prompts</strong></h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <%= image_tag ("avatar.jpg"), :size => "400"%>
                    </div>
                    <div class="col-md-4">
                        <%= image_tag ("apple_iphone-12-spring21_purple_04202021_big.jpg.large.jpg"), :size => "400"%>
                    </div>
                    <div class="col-md-4">
                        <%= image_tag ("image4.jpg"), :size => "400"%>
                    </div>
                </div>
            </div>
        </section>
    </main>

    <footer class="page-footer">
        <div class="container">
            <div class="links"></div>
            <div class="social-icons">
                <a href="https://discord.gg/YmxbzC79">
                    <i class="icon ion-social-discord"></i>
                </a>
                <%# <a href="#">
                    <i class="icon ion-social-instagram-outline"></i>
                </a>
                <a href="#">
                    <i class="icon ion-social-twitter"></i>
                </a> %>
            </div>
        </div>
    </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.6.1/pikaday.min.js"></script>
    <script src="app/assets/javascript/channels/bootstrap.min.js"></script>
    <script src="app/assets/javascript/packs/application.js"></script>
    
</body>
</html>

